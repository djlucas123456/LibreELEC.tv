#!/bin/sh
. /etc/profile
oe_setup_addon service.deluge

if [ -f "{$ADDON_HOME}/init.sh" ]; then
  sh "${ADDON_HOME}/init.sh"
fi

LD_LIBRARY_PATH="${ADDON_DIR}/libs:${LD_LIBRARY_PATH}" \
PYTHONPATH="${ADDON_DIR}/libs:${PYTHONPATH}" \
nice -n "${dl_nice}" \
python "${ADDON_DIR}/libs/bin/deluged" -c "${ADDON_HOME}" -d
